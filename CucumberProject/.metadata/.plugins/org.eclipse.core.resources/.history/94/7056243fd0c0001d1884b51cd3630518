package StringsHandling;

public class Practise2 {
	
	String str;
	
	public void stringreverse(String str)
	{
		this.str=str;
		System.out.println("Given String is :"+str);
		String rstr="";
		
		for (int i=str.length()-1;i>=0;i--)
		{
			rstr=rstr+str.charAt(i);
		}
		System.out.println(rstr);
	}

}
